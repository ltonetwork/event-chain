    "description": "Event chain service",
    "require": {
        "php": ">=5.4.0",
        "ext-json": "*",
        "ext-mongo": "*",
        "ext-libsodium": "1.*",
        "jasny/config" : "^1.1.2",
        "jasny/db-mongo" : "^1.2.13",
        "jasny/mvc": "2.0.0-beta1",
        "jasny/php-functions" : "^3.0",
