        "jasny/validation-result" : "^1.0.0",
        "jasny/auth": "dev-master as 0.1",
        "jasny/view": "^1.1.0",
        "jasny/error-handler": "^0.2.0",
        "jasny/social": "^0.4.3",
        "jasny/formbuilder": "~0.1.0",
        "jasny/assetic-extensions": "^1.0",
        "monolog/monolog": "1.*",
        "twig/twig": ">=1.10",
